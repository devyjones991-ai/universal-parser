  "wildberries_search": {
    "name": "Wildberries Search",
    "base_url": "https://www.wildberries.ru",
    "search_url": "https://search.wb.ru/exactmatch/ru/common/v4/search",
    "item_url": "https://www.wildberries.ru/catalog/{id}/detail.aspx",
    "method": "api",
    "params": {
      "appType": "1",
      "curr": "rub",
      "dest": "-1257786",
      "query": "{search_term}",
      "resultset": "catalog",
      "sort": "popular",
      "spp": "27",
      "suppressSpellcheck": "false",
      "page": "{page}",
      "limit": "100"
    },
    "data_path": "data.products",
    "selectors": {
      "title": "name",
      "price": "priceU",
      "old_price": "priceU",
      "rating": "reviewRating",
      "reviews_count": "feedbacks",
      "stock": "totalQuantity",
      "images": "pics",
      "description": "description",
      "brand": "brand",
      "category": "subj_name",
      "seller": "supplier",
      "seller_rating": "supplierRating",
      "discount": "sale",
      "colors": "colors",
      "sizes": "sizes",
      "characteristics": "nomenclatures",
      "delivery": "delivery",
      "pickup": "pickup",
      "warranty": "warranty"
    },
    "headers": {
      "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36",
      "Accept": "application/json, text/plain, */*",
      "Accept-Language": "ru-RU,ru;q=0.9,en;q=0.8",
      "Referer": "https://www.wildberries.ru/",
      "Origin": "https://www.wildberries.ru"
    },
    "wait_for": null,
    "timeout": 30,
    "use_browser": false,
    "categories": {
      "electronics": "Электроника",
      "clothing": "Одежда",
      "shoes": "Обувь",
      "home": "Дом и сад",
      "beauty": "Красота и здоровье",
      "sports": "Спорт и отдых",
      "auto": "Автотовары",
      "kids": "Детские товары",
      "books": "Книги",
      "food": "Продукты питания"
    },
    "filters": {
      "price_min": "priceU",
      "price_max": "priceU",
      "brand": "brand",
      "rating": "reviewRating",
      "discount": "sale",
      "in_stock": "totalQuantity",
      "delivery": "delivery",
      "pickup": "pickup"
    }
  },
